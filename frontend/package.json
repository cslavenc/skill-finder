{
  "scripts": {
    "postinstall": "ngcc",
    "ng": "ng",
    "prestart": "check-audit --production --audit-level=high && npm run generate:api",
    "start": "ng serve",
    "prebuild": "npm run generate:api",
    "build": "ng run beto-ui:ngsscbuild:production",
    "test": "ng test -c ci",
    "lint": "npx prettier --write . && ng lint",
    "generate:api": "openapi-generator-cli generate -g typescript-angular -i ../backend/src/main/resources/openapi.yaml -o generated/openapi",
    "resolve-audit": "resolve-audit --production --audit-level=high"
  },
  "name": "skillfinder-ui",
  "version": "0.0.1",
  "files": [
    "dist/*"
  ],
  "prettier": {
    "singleQuote": true,
    "printWidth": 130,
    "endOfLine": "crlf",
    "trailingComma": "none"
  },
  "config": {
    "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": true
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.2",
